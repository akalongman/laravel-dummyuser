{
    "name": "longman/laravel-dummyuser",
    "type": "library",
    "description": "Package to implement dummy user authorization (without database) for Laravel 5.x",
    "keywords": ["user", "auth", "laravel", "dbless"],
    "license": "MIT",
    "homepage": "https://github.com/akalongman/laravel-dummyuser",
    "support": {
        "issues": "https://github.com/akalongman/laravel-dummyuser/issues",
        "source": "https://github.com/akalongman/laravel-dummyuser"
    },
    "authors": [
        {
            "name": "Avtandil Kikabidze aka LONGMAN",
            "email": "akalongman@gmail.com",
            "homepage": "http://longman.me",
            "role": "Maintainer, Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/yaml": "~2.1|~3.0",
        "illuminate/console": "5.*",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "illuminate/http": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8|~5.7",
        "longman/php-code-style": "^1.0",
        "graham-campbell/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Longman\\LaravelDummyUser\\": "src/DummyUser"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
